<module>
    <inherits name="com.google.gwt.core.Core"/>
    <inherits name="com.google.gwt.user.UserAgent"/>
    
    <replace-with class="com.google.gwt.widgetideas.graphics.client.impl.GWTCanvasImplDefault">
    	<when-type-is class="com.google.gwt.widgetideas.graphics.client.impl.GWTCanvasImpl"/>
  	</replace-with>
  
  
    <replace-with class="com.google.gwt.widgetideas.graphics.client.impl.GWTCanvasImplIE6">
        <when-type-is class="com.google.gwt.widgetideas.graphics.client.impl.GWTCanvasImpl"/>
        <when-property-is name="user.agent" value="ie6"/>
  	</replace-with>
  	
    <source path="graphics/client/"/>
</module>
