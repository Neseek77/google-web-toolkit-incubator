<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User' />

	<!--  Inherit picker components-->
	<inherits name='com.google.gwt.gen2.picker.Picker' />

	<!-- Specify the app entry point class.                   -->
	<entry-point
		class='com.google.gwt.gen2.demo.picker.client.PickerDemo' />
	<stylesheet src='PickerDemo.css' />

	<!-- used to debug this widget's css -->
	<inherits name='com.google.gwt.gen2.widgetbase.Css_debug' />

</module>