<module>
  <!--  Inherit Logging -->
  <inherits name='com.google.gwt.gen2.logging.Logging' />

  <!-- Inherit the custom list box code   -->
  <inherits name='com.google.gwt.gen2.selection.Selection' />

  <!-- Specify the app entry point class.                         -->
  <entry-point
    class='com.google.gwt.gen2.demo.dropdownlistbox.client.DropDownListBoxDemo' />

  <!-- Inherit default gwt style sheet -->
  <inherits name="com.google.gwt.user.theme.standard.Standard" />

  <!-- Specify the application specific style sheet.              -->
  <stylesheet src='DropDownListBoxDemo.css' />

  <!-- used to debug this widget's css -->
 <inherits name='com.google.gwt.gen2.widgetbase.Css_debug' />  
</module>
