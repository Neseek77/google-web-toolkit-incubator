<module>
	<!--  Ensures user is included first -->
	<inherits name="com.google.gwt.user.User" /> 

  <!-- Include gwt-incubator common widget code -->
  <inherits name="com.google.gwt.gen2.commonwidget.CommonWidget" />

	<!--  Sets up source path -->
	<source path="shared" />
	<source path="client" />

	<!--  Sets up remote logging path -->
	<servlet path='/logging'
		class='com.google.gwt.gen2.logging.server.RemoteLoggingService' />

</module>