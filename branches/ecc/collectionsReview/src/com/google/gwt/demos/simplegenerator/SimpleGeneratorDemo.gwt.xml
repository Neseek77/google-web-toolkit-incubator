<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.widgetideas.WidgetIdeas'/>
  
 	<!-- Specify the app entry point class.                   -->
	<entry-point class='com.google.gwt.demos.simplegenerator.client.SimpleGeneratorDemo'/>
  
  <generate-with class="com.google.gwt.demos.simplegenerator.rebind.SimpleGenerator">
    <when-type-assignable class="com.google.gwt.demos.simplegenerator.client.SimpleGeneratorInterface" />
  </generate-with>
</module>
